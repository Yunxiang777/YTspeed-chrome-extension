{
  "manifest_version": 3,
  "name": "YouTube Speed Control",
  "version": "1.0",
  "description": "Control YouTube video playback speed.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/youtube_speed.png"
    }
  },
  "icons": {
    "16": "images/youtube_speed.png"
  }
}
